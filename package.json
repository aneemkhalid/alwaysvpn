{
  "name": "underscores",
  "version": "1.0.0",
  "description": "Hi. I'm a starter theme called _s, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "@wordpress/scripts": "^19.2.2",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-css-loader": "^4.1.1",
    "cross-env": "^7.0.3",
    "dir-archiver": "^1.2.0",
    "laravel-mix": "^6.0.39",
    "laravel-mix-clean-css": "^1.3.1",
    "laravel-mix-purgecss": "^6.0.0",
    "lottie-web": "^5.8.1",
    "minimist": "^1.2.5",
    "rtlcss": "^2.5.0",
    "sass-loader": "^12.1.0",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=production npx mix watch",
    "compile:css": "npx mix --production",
    "build": "npx mix --production",
    "start": "wp-scripts start",
    "build-js": "wp-scripts build",
    "watch:brad": "cross-env NODE_ENV=production npx mix watch -- --env --user=brad",
    "watch:edward": "cross-env npx mix watch -- --env --user=edward",
    "watch:jessi": "cross-env NODE_ENV=production npx mix watch -- --env --user=jessi",
    "watch:ryan": "cross-env NODE_ENV=production npx mix watch -- --env --user=ryan",
    "watch:syed": "cross-env NODE_ENV=production npx mix watch -- --env --user=syed"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "datatables.net-dt": "^1.11.5",
    "datatables.net-fixedcolumns-dt": "^4.0.2",
    "flickity": "^3.0.0",
    "inputmask": "^5.0.5",
    "lottie-web": "^5.8.1",
    "popper.js": "^1.16.1"
  }
}
